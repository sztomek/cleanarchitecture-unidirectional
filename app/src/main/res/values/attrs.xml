<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LoadingRetryView">
        <attr name="loadingMessage" format="reference|string"/>
        <attr name="retryMessage" format="reference|string"/>
        <attr name="showLoadingMessage" format="boolean"/>
        <attr name="showRetryMessage" format="boolean"/>
        <attr name="state" format="enum">
            <enum name="idle" value="1"/>
            <enum name="loading" value="2"/>
            <enum name="retry" value="3"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="SwipeRefreshWithText">
        <attr name="message" format="reference|string"/>
        <attr name="showText" format="boolean"/>
    </declare-styleable>
</resources>